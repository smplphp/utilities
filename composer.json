{
  "name": "smplphp/skeleton",
  "description": "A simple modern PHP skeleton library",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Ollie Read",
      "email": "smpl@ollie.codes"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "sempro/phpunit-pretty-print": "^1.4",
    "vimeo/psalm": "^4.21",
    "phpstan/phpstan": "^1.4",
    "infection/infection": "^0.26.9"
  },
  "autoload": {
    "psr-4": {
      "Smpl\\Skeleton\\": "src/",
      "Smpl\\Meta\\Skeleton\\": "meta/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smpl\\Skeleton\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
